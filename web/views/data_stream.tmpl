<h1 class="data">Stream: {{iden}}</h1>
<p>
    <strong>Connected:</strong> {{uri}}
    <br />
    <strong>Queue:</strong> {{queue}}
    <br />
    Number of messages received in the last minute: {{val}}
</p>
% for item in msgs:
<div class="code">
    <code>{{item}}</code>
</div>
% end
<form action="/data/stream/delete/{{iden}}" method="post">
    <input type="submit" value="Delete" onClick="return confirm('Sure?')"/>
</form>
<p><small>Welcome user: {{uid}}</small></p>
%rebase base.tmpl title='Suricate - Data Stream'