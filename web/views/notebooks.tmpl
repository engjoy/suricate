<div class="header">
    <ul class="menu">
        <li class="bgred"><a href="/">Home</a></li>
        <li class="bgblue"><a href="/data">Data</a></li>
        <li class="bggreen"><a href="/analytics">Analytics</a></li>
        <li class="bgyellow"><a href="/processing">Processing</a></li>
    </ul>
</div>
<div class="content">
    <h3>{{name}}</h3>
    <ul>
    % for ntb in notebooks:
      <li><a href="/{{name}}/{{ntb}}">{{ntb}}</a></li>
    % end
    </ul>
    <p>
        <form action="/{{name}}/upload" method="post" enctype="multipart/form-data">
            Name: <input type="text" name="iden" />
            (Optional) Select a file: <input type="file" name="upload" />
            <input type="submit" value="New" />
        </form>
    </p>
    <p><small>Welcome user: {{uid}}</small></p>
</div>
%rebase base.tmpl title='Suricate - Notebooks'